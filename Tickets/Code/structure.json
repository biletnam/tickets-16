    "query": {
        "from": {
            "table": "Tickets\\Tickets\\Code\\Tables\\Tickets",
            "alias": "tt",
            "name": "#__tickets_tickets"
        },
        "select": [
            "tt.id",
            "tt.title",
            "tt.category_id",
            "tc.title AS category_id_title",
            "tt.department_id",
            "td.title AS department_id_title",
            "tt.name",
            "tt.username",
            "tt.email",
            "tt.phone",
            "tt.description",
            "tt.created_by",
            "tt.date_created",
            "tt.modified_by",
            "tt.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Tickets\\Categories\\Code\\Tables\\Categories",
                "alias": "tc",
                "name": "#__tickets_categories",
                "on": "tt.category_id = tc.id"
            },
            {
                "table": "Tickets\\Departments\\Code\\Tables\\Departments",
                "alias": "td",
                "name": "#__tickets_departments",
                "on": "tt.department_id = td.id"
            }
        ]
    }

    "query": {
        "from": {
            "table": "Tickets\\Teams\\Code\\Tables\\Teams",
            "alias": "tt",
            "name": "#__tickets_teams"
        },
        "select": [
            "tt.id",
            "tt.name",
            "tt.address",
            "tt.email",
            "tt.department_id",
            "td.title AS department_id_title",
            "tt.user_id",
            "uu.name AS user_id_name",
            "uu.email AS user_id_email",
            "tt.published",
            "tt.created_by",
            "tt.date_created",
            "tt.modified_by",
            "tt.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Tickets\\Departments\\Code\\Tables\\Departments",
                "alias": "td",
                "name": "#__tickets_departments",
                "on": "tt.department_id = td.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "tt.user_id = uu.id"
            }
        ]
    }
